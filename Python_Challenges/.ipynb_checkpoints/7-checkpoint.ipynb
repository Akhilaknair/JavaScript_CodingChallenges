    "def print_matrix(n):\n",
    "    for i in range(n):\n",
    "        for j in range(n):\n",
    "            if i==j :\n",
    "                print(0,end =\" \")\n",
    "            else:\n",
    "                print(1,end=\" \")\n",
    "        print()\n",
    "\n",
    "n=int(input(\"enter value of n : \"))\n",
    "print_matrix(n)"
