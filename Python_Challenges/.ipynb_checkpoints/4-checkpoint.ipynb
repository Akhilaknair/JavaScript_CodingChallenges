   "source": [
    "def sumofdig(n):\n",
    "    sum=0\n",
    "    while n!=0:\n",
    "        sum=sum+(n%10)\n",
    "        n=n//10\n",
    "    print(sum)    \n",
    "n=int(input(\"enter a digit\"))\n",
    "sumofdig(n)"
   ]
